#include<iostream>
#include<conio.h>
using namespace std;
main()
{
	int x;
	cin>>x;
	switch(x){
		case 1:
			cout<<"it is saturday..it is a work day";
			break;
		case 2:
			cout<<"it is sunday..it is a work day";
			break;
		case 3:
			cout<<"it is monday..it is a work day";
			break;
		case 4:
			cout<<"it is tuesday..it is a work day";
			break;
		case 5:
			cout<<"it is wednesday..it is a work day";
			break;
		case 6:
			cout<<"it is thursday..it is a holiday:)))";
			break;
		case 7:
			cout<<"it is friday..it is a holiday:)))";
			break;
		default:
			cout<<"you enter e rong number";
			
			
	}
}
